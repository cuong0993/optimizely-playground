<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>disable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Using Include="EPiServer" />
    <Using Include="EPiServer.Core" />
    <Using Include="EPiServer.DataAbstraction" />
    <Using Include="EPiServer.DataAnnotations" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="EPiServer.CloudPlatform.Cms" Version="1.4.1" />
    <PackageReference Include="EPiServer.CMS.AspNetCore.HtmlHelpers" Version="12.20.2" />
    <PackageReference Include="EPiServer.CMS.AspNetCore.TagHelpers" Version="12.20.3" />
    <PackageReference Include="EPiServer.CMS.TinyMce" Version="4.7.1" />
    <PackageReference Include="EPiServer.CMS.UI" Version="12.27.0" />
    <PackageReference Include="EPiServer.CMS.UI.AspNetIdentity" Version="12.27.0" />
    <PackageReference Include="EPiServer.CMS.UI.VisitorGroups" Version="12.27.0" />
    <PackageReference Include="EPiServer.Hosting" Version="12.20.2" />
    <PackageReference Include="EPiServer.ImageLibrary.ImageSharp" Version="2.0.1" />
    <PackageReference Include="Wangkanai.Detection" Version="8.11.0" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\Translations\**\*" />
  </ItemGroup>
</Project>
